@article{Kahn1997,
abstract = {The use of infrared radiation as a medium for high-speed$\backslash$nshort-range wireless digital communication is discussed. Available$\backslash$ninfrared links and local-area networks are described. Advantages and$\backslash$ndrawbacks of the infrared medium are compared to those of radio and$\backslash$nmicrowave media. The physical characteristics of infrared channels using$\backslash$nintensity modulation with direct detection (IM/DD) are presented$\backslash$nincluding path losses and multipath responses. Natural and artificial$\backslash$nambient infrared noise sources are characterized. Strategies for designs$\backslash$nof transmitter and receivers that maximize link signal-to-noise ratio$\backslash$n(SNR) are described. Several modification formats are discussed in$\backslash$ndetail, including on-off keying (OOK) pulse-position modulation (PPM),$\backslash$nand subcarrier modulation. The performance of these techniques in the$\backslash$npresence of multipath distortion is quantified. Techniques for$\backslash$nmultiplexing the transmissions of different users are reviewed. The$\backslash$nperformance of an experimental 50-Mb/s on-off-keyed diffuse infrared$\backslash$nlink is described},
author = {Kahn, Joseph M.},
doi = {10.1109/5.554222},
